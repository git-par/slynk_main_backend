{
  "name": "slynk-backend",
  "version": "1.0.0",
  "description": "\"# slynk-backend\"",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "postinstall": "tsc",
    "heroku": "node dist/server.js",
    "pm2-staging": "pm2 start npm --name staging-dc -- run staging-server",
    "build": "tsc",
    "watch": "ts-node-dev --respawn --transpile-only src/server.ts  config:env/dev.env",
    "dev-build": "node dist/server.js  config:env/dev.env",
    "dev": "ts-node src/server.ts  config:env/dev.env",
    "prod": "ts-node src/server.ts  config:env/dc-production.env",
    "prod-js": "tsc & node dist/server.js  config:env/dc-production.env",
    "prod-ec2": "node dist/server.js  config:/var/growlytics/appi.env",
    "staging": "ts-node src/server.ts config:env/dc-staging.env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parthgodhani/slynk-backend.git"
  },
  "author": "parth godhani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parthgodhani/slynk-backend/issues"
  },
  "homepage": "https://github.com/parthgodhani/slynk-backend#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/geoip-lite": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.16.1",
    "@sentry/node": "^6.18.1",
    "@sentry/tracing": "^6.18.1",
    "@types/busboy": "^0.3.1",
    "@types/crypto-js": "^4.0.2",
    "@types/ejs": "^3.1.0",
    "@types/lodash": "^4.14.178",
    "@types/validator": "^13.7.2",
    "agenda": "^4.3.0",
    "apn": "^2.2.0",
    "axios": "^0.27.2",
    "busboy": "^1.0.0",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.6",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "ejs-lint": "^1.2.1",
    "express": "^4.17.2",
    "express-http-context": "^1.2.4",
    "express-winston": "^4.2.0",
    "firebase-admin": "^11.10.1",
    "generate-password": "^1.7.0",
    "geoip-lite": "^1.4.6",
    "google-auth-library": "^7.11.0",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mongoose": "^6.1.3",
    "mongoose-autopopulate": "^0.16.0",
    "nodemailer": "^6.7.2",
    "sib-api-v3-sdk": "^8.3.0",
    "socket.io": "^4.7.2",
    "stripe": "^8.209.0",
    "ts-node-dev": "^1.1.8",
    "twilio": "^3.76.0",
    "validator": "^13.7.0",
    "web-push": "^3.6.6",
    "winston": "^3.8.0"
  }
}
